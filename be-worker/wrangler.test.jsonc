/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "be-worker",
	"main": "src/index.ts",
	"compatibility_date": "2025-11-21",
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
  // "ai": { "binding": "AI" }, // Commented out for tests to avoid remote dependency
	"triggers":
	{
		"crons": [
			"*/5 * * * *"
		]
	},
  "d1_databases": [
    {
      "binding": "db",
      "database_name": "db",
      "database_id": "25f6cd4b-a401-4c6a-b922-9feb4b5b9ad9"
    }
  ]
}
